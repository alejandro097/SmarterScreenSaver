# Screensaver Application Configuration
# Time in milliseconds before triggering screensaver when idle

IDLE_THRESHOLD_MS = 120000

# Common values:
# 30 seconds:    IDLE_THRESHOLD_MS = 30000  
# 1 minute:      IDLE_THRESHOLD_MS = 60000
# 2 minutes:     IDLE_THRESHOLD_MS = 120000
# 5 minutes:     IDLE_THRESHOLD_MS = 300000
# 10 minutes:    IDLE_THRESHOLD_MS = 600000